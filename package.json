{
  "name": "openwork",
  "version": "0.1.0",
  "private": true,
  "description": "A Claude Cowork-like application powered by OpenCode",
  "workspaces": [
    "ui",
    "bridge"
  ],
  "scripts": {
    "logs:prepare": "mkdir -p logs/ui logs/bridge logs/opencode",
    "dev": "npm run logs:prepare && concurrently \"npm run dev:bridge\" \"npm run dev:ui\"",
    "dev:ui": "npm run logs:prepare && npm run dev --workspace=ui 2>&1 | tee -a logs/ui/dev.log",
    "dev:bridge": "npm run logs:prepare && npm run dev --workspace=bridge 2>&1 | tee -a logs/bridge/dev.log",
    "dev:opencode": "npm run logs:prepare && opencode serve --port 4096 --hostname 127.0.0.1 2>&1 | tee -a logs/opencode/dev.log",
    "build": "npm run build --workspaces",
    "clean": "rm -rf node_modules ui/node_modules bridge/node_modules ui/dist bridge/dist"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
